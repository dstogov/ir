--TEST--
002: Swap operands of commutative instruction
--TARGET--
Windows-x86_64
--ARGS--
-S
--CODE--
{
	l_1 = START(l_5);
	int32_t x = PARAM(l_1, "x", 1);
	uintptr_t v = PARAM(l_1, "ptr", 3);
	int32_t z, l_3 = LOAD(l_1, v);
	int32_t ret = AND(z, x);
	int32_t ret2 = ADD(ret, ret);
	l_4 = STORE(l_3, v, ret2);
	l_5 = RETURN(l_4);
}
--EXPECT--
test:
	andl (%rsi), %edi
	leal (%rdi, %rdi), %eax
	movl %eax, (%rsi)
	retq
