--TEST--
001: if function
--TARGET--
aarch64
--ARGS--
-O1 -S
--CODE--
{
	int32_t c_1 = 0;
	int32_t c_2 = 42;
	l_1 = START(l_8);
	int32_t x = PARAM(l_1, "x", 1);
	l_2 = IF(l_1, x);
	l_3 = IF_TRUE(l_2);
	l_4 = END(l_3);
	l_5 = IF_FALSE(l_2);
	l_6 = END(l_5);
	l_7 = MERGE(l_4, l_6);
	int32_t ret = PHI(l_7, c_1, c_2);
	l_8 = RETURN(l_7, ret);
}
--EXPECT--
test:
