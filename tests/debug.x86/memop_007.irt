--TEST--
007: Memory update (shift const)
--TARGET--
x86
--ARGS--
-S
--CODE--
{
	int32_t c = 3;
	l_1 = START(l_5);
	int32_t y = PARAM(l_1, "y", 1);
	uintptr_t v = PARAM(l_1, "ptr", 2);
	int32_t z, l_3 = LOAD(l_1, v);
	int32_t ret = SHL(z, c);
	l_4 = STORE(l_3, v, ret);
	l_5 = RETURN(l_4);
}
--EXPECT--
test:
	movl 8(%esp), %eax
	shll $3, (%eax)
	retl
