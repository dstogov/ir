--TEST--
003: Memory update (binary op with reg + swap)
--TARGET--
x86_64
--ARGS--
-S
--CODE--
{
	int32_t c = 0x80;
	l_1 = START(l_5);
	int32_t y = PARAM(l_1, "y", 1);
	uintptr_t v = PARAM(l_1, "ptr", 2);
	int32_t z, l_3 = LOAD(l_1, v);
	int32_t y2 = ADD(y, y);
	int32_t ret = AND(y2, z);
	l_4 = STORE(l_3, v, ret);
	l_5 = RETURN(l_4);
}
--EXPECT--
test:
	leal (%rdi, %rdi), %eax
	andl %eax, (%rsi)
	retq
