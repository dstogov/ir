--TEST--
001: Memory update (binary op with const)
--TARGET--
x86_64
--ARGS--
-S
--CODE--
{
	int32_t c = 0x80;
	l_1 = START(l_5);
	uintptr_t v = PARAM(l_1, "ptr", 1);
	int32_t z, l_3 = LOAD(l_1, v);
	int32_t ret = AND(z, c);
	l_4 = STORE(l_3, v, ret);
	l_5 = RETURN(l_4);
}
--EXPECT--
test:
	andl $0x80, (%rdi)
	retq
